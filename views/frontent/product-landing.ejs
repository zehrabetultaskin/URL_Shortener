<%-include('common/head.ejs')-%>

<%for (i = 0; i < 1; i++) {%>   
  <title><%=a.categoryTitle%></title>
  <meta name="description" content="<%=a.categoryMeta%>" />
  <meta name="keywords" content="<%=a.categoryDesc%>" />
<%}%> 





   </head>
   <body>
      <%-include('common/mobile-nav.ejs')-%>
      <%-include('common/header.ejs')-%>


      <section class="teach-coders-dtls BasicJavaScript landingpage">
         <div class="container">
          
            <%for (i = 0; i < 1; i++) {%>   
              <h1 style="font-size:30px;"><%=x[i].productCat%></h1>              
            <%}%> 
            <br><br>
            
              
            <div class="row">
               <%x.forEach((y)=>{%>                  
               <div class="col-sm-4">
                 <div class="card mb-4">
                   <div class="card-body">
                     <h5 class="card-title"><%=y.productHeading%></h5>
                     <p class="card-text"><%-y.productDetails.slice(0, 250)-%></p><br>
                     <a href="/course/<%=y.productCat%>/<%=y.productUrl%>" class="btn btn-primary">View More</a>
                   </div>
                 </div>
               </div>
               <%})%>              
             </div>
         </div>
      </section>
      <%-include('common/footer.ejs')-%>